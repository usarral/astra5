{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"baseUrl": "./src",
		"moduleResolution": "Node",
		"outDir": "dist",
		"importsNotUsedAsValues": "remove",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true   
	},
	"include": ["src/**/*", "../server-base.ts"],
	"exclude": ["node_modules"]
}